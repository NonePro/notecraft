{
	"typescript.tsc.autoDetect": "off",
	"search.exclude": {
		"media/webview.css": true,
		"media/webview.js": true,
		"media/webview.js.map": true,
		"out": true,
		"dist": true,
		"media/vendor": true
	},
	"eslint.workingDirectories": [
		"./",
		"media"
	],
	"editor.formatOnSave": false,
	"contributions.settings.moveOutPrefix": true,
	"contributions.commands.excludeById": [
		"notecraft.dev.setLastVisit",
		"notecraft.dev.clearGlobalState",
		"notecraft.dev.showGlobalState",
		"notecraft.dev.removeAllOverdue",
		"notecraft.addTaskToActiveFileWebview",
		"notecraft.mainStatusBarCommand"
	],
	"contributions.settings.replaceDefaultValue": {
		"notecraft.webview.fontFamily": "\"...\"",
		"notecraft.mainStatusBarItem": "{...}",
		"notecraft.progressStatusBarItem": "{...}",
		"notecraft.decorations": "{...}"
	},
	"cSpell.words": [
		"Doesnt",
		"notecraft",
		"ungroup",
		"zettelkasten"
	]
}
